<?xml version="1.0"?>


<robot xmlns:xacro="http://ros.org/wiki/xacro" name="raksh">

    <xacro:include filename="$(find raksh_description)/urdf/material.xacro"/>
    <xacro:include filename="$(find raksh_description)/urdf/raksh_description.urdf.xacro"/>

    <link name="base_link"/>
    <joint name="chasis_joint" type="fixed">
        <origin xyz="-0.25 0 0" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="chassis"/>
    </joint>

</robot>